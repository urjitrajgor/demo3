<!DOCTYPE html>
<html>
  <head>
    <title>Demo1</title>
    <%= stylesheet_link_tag 'markitup.css','style.css', 'feedzirra.css', 'jquery-ui-1.7.3.custom.css' %>
    <%= javascript_include_tag  'jquery.js', 'jquery.markitup.js', 'set.js','jquery-ui-1.7.3.custom.min.js', 'jquery-1.3.2.min.js'%>
    <%= javascript_include_tag :defaults %>
    
    <%= csrf_meta_tag %>
  </head>
  <body>
    <%  if user_signed_in? %>
      <%= link_to "Home", root_path %> |
      <%= link_to "signout", logout_path %> |
      <%= link_to "Dashboard", dashboard_path %>
    <%end%>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>


    <%= yield %>

  </body>
</html>
